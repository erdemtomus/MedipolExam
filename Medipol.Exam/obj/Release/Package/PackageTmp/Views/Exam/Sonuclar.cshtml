@using Medipol.Exam.Models
@model List<TakenExam>
@{
    ViewBag.Title = "Sonuclar";
}

<h2>Sonuclar</h2>

<table>
    <thead>
        <tr>
            <th>Kullanıcı</th>
            <th>Skor</th>
            <th>Durum</th>
            <th>Tarih</th>
        </tr>
    </thead>
    <tbody>
        @foreach (TakenExam item in Model)
        {
            <tr>
                <td>@item.Username</td>
                <td>@item.Score</td>
                <td>@item.Status</td>
                <td>@item.TakenDate</td>
            </tr>
        }
    </tbody>
</table>